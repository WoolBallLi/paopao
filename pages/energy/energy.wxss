page{
  height: 100%;
  width: 100%;
  background-color: #E9E9E9;
}
.energy_logo{
  height: 315rpx;
  position: relative;
  overflow: hidden;
}
.energy_logo image:nth-of-type(1){
  height: 272rpx;
  width: 284rpx;
  position: absolute;
  top: 43rpx;
  left: 234rpx;
}
.energy_logo image:nth-of-type(2){
  height: 120rpx;
  width: 274rpx;
  position: absolute;
  top: 11rpx;
  right: 8rpx;
}
.energy_img{
  height: 628rpx;
  width: 690rpx;
  margin-left: 30rpx;
  border-radius:20rpx;
