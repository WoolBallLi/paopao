}
.energy_img image{
  /* position: absolute; */
}
.energy_box{
  height: 314rpx;
  width: 230rpx;
  background-color: #fff;
  float: left;
  box-sizing: border-box;
  position: relative;
}
.energy_box text{
  font-size: 60rpx;
  color: #7a7a7a;
  position: absolute;
  top: 216rpx;
  left: 118rpx;
  
}

.energy_box image{
  position: absolute;
}

.energy_box:nth-of-type(1){
  border-radius:20rpx 20rpx 0 20rpx;
  border-right:2rpx dashed #EFEFEF;
  border-bottom:2rpx dashed #EFEFEF;    
}
.energy_box:nth-of-type(2){
  border-radius:20rpx 20rpx 0 0;
  border-right:2rpx dashed #EFEFEF;   
  border-bottom:2rpx dashed #EFEFEF;    
}
.energy_box:nth-of-type(3){
  border-bottom:2rpx dashed #EFEFEF;    
  border-radius:20rpx 20rpx  20rpx 0; 
}
.energy_box:nth-of-type(4){
  border-radius:20rpx 0 20rpx  20rpx; 
  border-right:2rpx dashed #EFEFEF;   
}
.energy_box:nth-of-type(5){
  border-right:2rpx dashed #EFEFEF;   
  border-radius:0 0 20rpx  20rpx; 
}
.energy_box:nth-of-type(6){
  border-radius:0 20rpx 20rpx 20rpx; 
}
