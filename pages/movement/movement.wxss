/* movement.wxss */
page{
  font-family: "微软雅黑";
  height: 100%;
}
.d_name{
  font-size: 28rpx;
  color: #525252;
  margin-left: 8rpx;
}
.d_money{
  font-size: 32rpx;
}
.d_mark{
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, .4);
  z-index: 100;
  overflow: auto;
}
 .d_box{
  height: 696rpx;
  width: 596rpx;
  background-color:#fafafa; 
  border-radius: 16rpx;
  margin-top: 286rpx;
  margin-left: 78rpx;
  padding-left: 48rpx;
  box-sizing: border-box;
  position: relative;
  z-index: 999;
}
.d_box .d_title{
  font-size: 32rpx;
  color: #BDBDBD;
  margin-top: 12rpx;
  margin-bottom: 30rpx;
  display: inline-block;
}
.drinkcolor{
  color:#FB8C00;
}
.d_button{
  height: 60rpx;
  width: 120rpx;
  font-size: 26rpx;
  position: absolute;
  bottom: 26rpx;
  right: 44rpx;
  background-color: #FB8C00;
  color: #fff;
}
.header{
  height: 100rpx;
  width: 100%;
  padding-top: 40rpx;
  box-sizing: border-box;
  position: relative;
}
.header text{
  height: 100%;
  font-size: 34rpx;
  color: #030303;
  width: 160rpx;
}
.header .n_circle{
  margin-left: 108rpx;
  float: left;
  padding-left: 34rpx;
}
.header .color{
  color: #0D6EFF;
}
.header .m_circle{
  float: right;
  margin-right: 108rpx;
}
swiper{
  height: 340rpx;
  width: 100%;
}
swiper image{
  height: 100%;
  width: 100%;
}
.main{
  background-color: #e9e9e9;
  height: auto;
  width: 100%;
  margin-top:68rpx; 
  margin-bottom: 20rpx;
  overflow: hidden;
}
.main .near{
  text-align: center;
  font-size: 50rpx;
  color: #000000;
  padding-bottom: 66rpx;
  background-color: #fff; 
}
.main .movedata{
  background-color: #fff;
}
.main .movedata .movedataImg{
  height: 154.6rpx;
  width: 138rpx;
  margin-left: 306rpx;
  margin-top: 190rpx;
}
.main .user_circle{
  background-color: #fff;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.main .user_circle .userId{
  width: 80rpx;
  height: 80rpx;
  border-radius:50%;
  margin-left: 32rpx; 
  float: left;
  margin-bottom: 48rpx;
}
.main .user_circle .userName{
  font-size: 32rpx;
  margin:20rpx 11rpx 0 11rpx;
  float: left;
}
.main .user_circle .gender{
  width: 28rpx;
  height: 28rpx;
  float: left;
  margin-top:26rpx; 
}
.main .user_circle .time{
  margin-top: 72rpx;
  display: block;
  font-size: 24rpx;
  margin-left: 124rpx;
  color: #656565;
}
.main .user_circle .attention{
  float: right;
  width: 140rpx;
  height: 52rpx;
  display: inline-block;
  line-height: 52rpx;
  text-align: center;
  background-color: #41E8A1;
  color: #ffffff;
  border-radius: 6rpx;
  margin-right: 30rpx;
  margin-top: 8rpx;
  font-size: 32rpx;
}
.main .user_circle .textpage{
  padding-left: 26rpx;
  font-size: 32rpx;
  
}
.main .user_circle .textImage{
  margin-top: 44rpx;
  overflow: hidden;
}
.main .user_circle .textImage image{
  float: left;
  height: 240rpx;
  width: 246rpx;
}
.main .user_circle .textImage image:nth-of-type(1),.main .user_circle .textImage image:nth-of-type(2),.main .user_circle .textImage image:nth-of-type(3){
  margin-bottom: 4rpx;
}
.main .user_circle .textImage image:nth-of-type(2){
  margin:0 6rpx;
}

.main .user_circle .textImage image:nth-of-type(5){
  margin:0 6rpx;
}
.main .user_circle .Icon{
  height: 100rpx;
  width: 690rpx;
  box-sizing: border-box;
  margin: 0 30rpx;
  border-bottom: 2rpx solid #eeeeee;
}
.main .user_circle .Icon .locate{
  width: 26rpx;
  height: 32rpx;
  margin-top: 32rpx;
  margin-right: 10rpx;
  float: left;
}
.main .user_circle .Icon .km{
  font-size: 24rpx;
  float: left;
  margin-top: 34rpx;
  color: #a9a9a9;
}
.main .user_circle .Icon .Icon_right{
  float: right;
  position: relative;
}
.d_markBox{
  height: 90rpx;
  width: 50rpx;
  position: absolute;
  top: 0rpx;
  left: -4rpx;
}
.f_markBox{
  height: 90rpx;
  width: 60rpx;
  position: absolute;
  top: 0rpx;
  left: 70rpx;
}
.main .user_circle .Icon .Icon_right image{
  height: 46rpx;
  width: 20rpx;
  float: left;
  margin-top: 22rpx;
}
.main .user_circle .Icon .Icon_right image:nth-of-type(2){
  height: 32rpx;
  width: 28rpx;
  float:left;
  margin: 34rpx 60rpx 0 60rpx;
}
.main .user_circle .Icon .Icon_right button{
  height: 32rpx;
  width: 40rpx;
  float:left;
  margin-top: 34rpx;
}
.main .user_circle .Icon .Icon_right button image{
  height: 100%;
  width: 100%;
  margin: 0;
}
.main .user_circle .praise{
  margin: 0 30rpx;
  margin-bottom: 28rpx;
  overflow: hidden;
}

.main .user_circle .praise .flo{
  height: 32rpx;
  width: 28rpx;
  margin-top: 36rpx;
  float: left;
  margin-right: 20rpx;
}
.main .user_circle .praise .userOid{
  height: 56rpx;
  width: 56rpx;
  border-radius: 50%;
  float: left;
  margin-top:22.4rpx;
}
.main .user_circle .reply{
  font-size: 28rpx;
  margin: 0 30rpx;
  
}
.main .user_circle .reply .username{
  color: #0C5ACE;
}
.main .user_circle .reply .more{
  display: block;
  color: #838383;
  margin-top: 4rpx;
  margin-bottom: 20rpx; 
}
.main .user_circle .reply .write{
  height: 60rpx;
  width: 690rpx;
  box-sizing: border-box;
  border: 1px solid #dddddd;
  border-radius: 16rpx;
  padding-left: 20rpx;
  margin-bottom: 30rpx;
}
.flower_mark{
  height: 100%;
  width: 750rpx;
  background: rgba(0, 0, 0, .5);
  position: fixed;
  left: 0;
  top: 0;
  overflow: hidden;
  z-index: 100;
}
.flower_mark text{
  font-size: 40rpx;
  color: #fff;
  position: absolute;
  left: 196rpx;
  top: 368rpx;
}
.flower_mark image{
  height: 590rpx;
  width: 472rpx;
  position: absolute;
  top: 440rpx;
  left:140rpx;
}